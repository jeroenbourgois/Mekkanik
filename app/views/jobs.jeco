<h1>Jobs</h1>

<table id="joblist" class="table table-striped">
  <thead>
    <tr>
      <th>#</th>
      <th>Client</th>
      <th>Edit</th>
    </tr>
  </thead>
  <tbody>
  <% for job in @jobs: %>
    <tr>
      <td>id</td>
      <td><%= job.client %></td>
      <td><button class="btn edit" data="<%= job.id %>">edit</button></td>
    </tr>
  <% end %>
  </tbody>
</table>
